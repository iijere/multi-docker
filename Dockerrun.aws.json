{
    "AWSEBDockerrunVerion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "julianijere/kliqr:client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "julianijere/kliqr:server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "julianijere/kliqr:worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "julianijere/kliqr:nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "server"
            ],
            "memory": 128
        }
    ]
}